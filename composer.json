{
  "name": "texboy/vitalflix",
  "description": "Vitalflix backend",
  "require": {
    "pecee/simple-router":"*",
    "vlucas/phpdotenv": "^4.1",
    "monolog/monolog": "^2.0",
    "cweagans/composer-patches": "^1.6"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Vitalflix\\": "app/code/Vitalflix/",
      "Bootstrap\\": "app/bootstrap/",
      "Core\\": "app/code/Core/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "extra": {
    "patches": {
      "pecee/simple-router":{
        "Fix classloader not being used when instantiating custom exception handlers ": "patches/pecee-simple-router_fix-class-loader-not-being-used-forcustom-exception-handlers.patch"
      }
    }
  }
}